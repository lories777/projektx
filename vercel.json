{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/src/server.ts"
    }
  ],
  "buildCommand": "cd backend && npm install && npm run build",
  "outputDirectory": "backend/dist"
}
